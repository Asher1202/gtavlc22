	static bool:TatGhiDanh;
	CMD:ghidanh(playerid, params[]) {
		if(TatGhiDanh || GetPVarInt(playerid, "GhiDanhDelay") > gettime()) return 1;
		SetPVarInt(playerid, "GhiDanhDelay", gettime()+5);
		new query[200];
		format(query, sizeof(query), "SELECT * FROM ghidanh_boxing WHERE Userid=%d", PlayerInfo[playerid][pSQLID]);
		new Cache:x = mysql_query(SQL, query), rows = cache_num_rows(), k;
		cache_get_value_name_int(0, "id", k);
		cache_delete(x);
		if(rows > 0) {
			format(query, sizeof(query), "Ban da dang ky voi so bao danh la: %03d. \nHen gap ban tai cuoc thi.", k);
			Dialog_Show(playerid, 0, DIALOG_STYLE_MSGBOX, "Ghi danh boxing", query, "Chon", "");
		} 
		else {
			format(query, sizeof(query), "Ban chua dang ky thi dau boxing, de dang ky ban can chiu phi $500,000.\nDang ky ngay\nKhong phai bay gio.");
			Dialog_Show(playerid, GHIDANHBOXING, DIALOG_STYLE_TABLIST_HEADERS, "Ghi danh boxing", query, "Chon", "");
		}
		return 1;
	}
	Dialog:GHIDANHBOXING(playerid, response, listitem, inputtext[]) {
		if(listitem == 1 || !response) return 1;
		if(GetPlayerCash(playerid) < 500000) return SendClientMessage(playerid, -1, "Ban khong co du tien.");
		new query[200];
		mysql_format(SQL, query, sizeof(query), "INSERT INTO ghidanh_boxing (Userid, Type, Name) VALUES (%d,0,'%e')", PlayerInfo[playerid][pSQLID], GetName(playerid));
		new Cache: x = mysql_query(SQL, query), insertID = cache_insert_id();
		cache_delete(x);
		new Cache: a = mysql_query(SQL, "SELECT * FROM ghidanh_boxing"), rows = cache_num_rows();
		format(query, sizeof(query), "{f49842}GTAVLC Boxing Tournament{FFFFFF}\nBoxers: %d\nTong gia tri: $%s\nSu dung /ghidanh de ghi danh", rows, FormatNumber(rows*500000));
		cache_delete(a);
		UpdateDynamic3DTextLabelText(GhiDanhBoxingText, 0xFFFFFFFF, query);
		GivePlayerCash(playerid, -500000);
		Update(playerid, pCashx);
		format(query, sizeof(query), "Ban da ghi danh boxing thanh cong, so bao danh cua ban la: %d, hen gap lai.\nChao tam biet.", insertID);
		Dialog_Show(playerid, 0, DIALOG_STYLE_TABLIST_HEADERS, "Ghi danh boxing", query, "Chon", "");

		return 1;
	}
	CMD:bxmenu(playerid, params[]) {
		if(PlayerInfo[playerid][pAdmin] < 1) return 1;
		new info[450];
		format(info, sizeof(info), "Chon mot tuy chon\n%s ghi danh boxing\nKiem tra mot nguoi co ghi danh hay chua\nKiem tra mot nguoi co cai sampcac hay chua", !TatGhiDanh ? "Tat" : "Mo");
		Dialog_Show(playerid, BXMENU, DIALOG_STYLE_TABLIST_HEADERS, "Tuy chon boxing", info, "Chon", "Cancel");
		return 1;
	}
	Dialog:BXMENU(playerid, response, listitem, inputtext[]) {
		if(!response) return 1;
		switch(listitem) {
			case 0: {
				TatGhiDanh = !TatGhiDanh;
				new str[300];
				format(str, sizeof(str), "%s vua %s ghi danh boxing.", GetName(playerid), TatGhiDanh ? "tat" : "mo");
			}
			case 1: {
				Dialog_Show(playerid, BXMENU2, DIALOG_STYLE_INPUT, "Kiem tra nguoi choi ghi danh hay chua:", "Nhap ID nguoi do:", "Ok", "Thoat");
			}
			case 2: {
				Dialog_Show(playerid, BXMENU2, DIALOG_STYLE_INPUT, "Kiem tra nguoi choi da cai SAMPCAC hay chua:", "Nhap ID nguoi do:", "Ok", "Thoat");
			}
		}
		SelectedItem[playerid] = listitem;
		return 1;
	}
	Dialog:BXMENU2(playerid, response, listitem, inputtext[]) {
		if(!response) return 1;
		new y = strval(inputtext);
		if(!IsPlayerConnected(y)) return Dialog_Show(playerid, 0, DIALOG_STYLE_MSGBOX, "INFO", "Nguoi choi khong ket noi", "Ok", "");
		switch(SelectedItem[playerid]) {
			case 1: {
				new query[200];
				format(query, sizeof(query), "SELECT * FROM ghidanh_boxing WHERE Userid=%d", PlayerInfo[playerid][pSQLID]);
				new Cache:x = mysql_query(SQL, query), rows = cache_num_rows(), k;
				cache_get_value_name_int(0, "id", k);
				cache_delete(x);

				if(rows > 0) va_SendClientMessage(playerid, -1, "Nguoi choi %s co dang ky voi so bao danh: %03d", GetName(y), k);
				else SendClientMessage(playerid, -1, "Nguoi choi khong dang ky");
			}
			case 2: {
				if(UsingSampcac{y} == 1) return Dialog_Show(playerid, 0, DIALOG_STYLE_MSGBOX, "INFO", "Nguoi choi co su dung SAMPCAC", "Ok", "");
				else Dialog_Show(playerid, 0, DIALOG_STYLE_MSGBOX, "INFO", "Nguoi choi khong su dung SAMPCAC", "Ok", "");
			}
		}
		return 1;
	}
